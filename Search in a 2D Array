//Codestudio Question Link -> https://www.codingninjas.com/studio/problems/search-in-a-2d-matrix_8230773?challengeSlug=striver-sde-challenge&leftPanelTab=1

bool searchMatrix(vector<vector<int>>& mat, int target) {
        for(int i=0;i<mat.size();i++){
            for(int j=0;j<mat[i].size();j++){
                if(mat[i][j]==target)     return target;
                
            }
        }
        return 0;
}
